{
    "openapi":"3.0.3",
    "info": {
      "title":"Forms Data Retrieval API",
      "version":"1.0"
    },
    "servers": [
        {
            "url":"https://api.forms-staging.cdssandbox.xyz"
        }
    ],
    "components": {
        "securitySchemes": {
            "accessToken": {
                "type": "http",
                "scheme": "bearer",
                "bearerFormat": "JWT"
            }
        }
