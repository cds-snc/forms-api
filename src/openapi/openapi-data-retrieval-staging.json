            "get": {
                "description": "",
                "parameters": [
                    {
                        "name": "formId",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "description": "The ID of the form",
                        "example": "cmfmmallr0001x00113h7f9bt"
                    }
                ],          
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/forms/{formId}/template": {
            "get": {
