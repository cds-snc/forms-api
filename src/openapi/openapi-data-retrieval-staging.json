                    }
                }
            }
        },
        "/forms/{formId}/submission/{submissionName}/confirm/{confirmationCode}": {
            "put": {
                "description": "",
                "parameters": [
                    {
                        "name": "formId",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "description": "The ID of the form",
                        "example": "cmfmmallr0001x00113h7f9bt"
                    },
                    {
                        "name": "submissionName",
